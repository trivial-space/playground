{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"esnext",
			"dom"
		],
		"module": "es6",
		"moduleResolution": "node",
		"strict": true,
		"noImplicitAny": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"sourceMap": true,
		"baseUrl": ".",
		"paths": {
			"*": [ "src/*" ]
		}
	},
	"exclude": ["node_modules"],
	"compileOnSave": false,
	"buildOnSave": false,
	"atom": {
		"rewriteTsconfig": false
	}
}
