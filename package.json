{
  "name": "tvs-playground",
  "version": "0.1.0",
  "description": "A collection of examples using flow, flow-editor, renderer and libs in action",
  "author": "Thomas Gorny",
  "license": "MIT",
  "homepage": "https://trivialspace.net",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trivial-space/playground.git"
  },
  "bugs": {
    "url": "https://github.com/trivial-space/playground/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "chokidar-socket-emitter": "^0.5.4",
    "eslint": "^3.12.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "jspm": "^0.17.0-beta.32",
    "tvs-flow": "trivial-space/flow",
    "tvs-libs": "trivial-space/libs",
    "tvs-renderer": "trivial-space/renderer",
    "typescript": "^2.1.4"
  },
  "jspm": {
    "name": "tvs-playground",
    "directories": {
      "baseURL": "public",
      "packages": "public/shared/js/vendor",
      "configFiles": "public/shared/js"
    },
    "configFiles": {
      "jspm": "public/shared/js/jspm.config.js"
    },
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.26",
      "geo-3d-box": "npm:geo-3d-box@^2.0.2",
      "gl-matrix": "npm:gl-matrix@^2.3.2",
      "text": "github:systemjs/plugin-text@^0.0.9",
      "ts": "github:frankwallis/plugin-typescript@^5.3.1",
      "tvs-flow": "github:trivial-space/flow@master",
      "tvs-libs": "github:trivial-space/libs@master",
      "tvs-renderer": "github:trivial-space/renderer@master"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.13",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.6.0"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "domain": "npm:jspm-nodelibs-domain@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "react": "npm:react@^15.3.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zlib": "npm:jspm-nodelibs-zlib@^0.2.0"
    },
    "overrides": {
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:typescript@2.1.4": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        },
        "dependencies": {
          "source-map-support": "*"
        }
      }
    }
  }
}
